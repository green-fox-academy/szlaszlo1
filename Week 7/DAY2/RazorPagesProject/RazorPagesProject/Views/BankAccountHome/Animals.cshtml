@model List<BankAccount>
@{
    ViewData["Title"] = "Animals";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Animals</h2>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table class="table table-bordered table-responsive table-hover">
        @foreach (BankAccount item in Model)
        {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Balance @item.Currency</td>
            <td>@item.AnimalType</td>
            <td>@item.King</td>
        </tr>
        }

    </table>
</body>
</html>

